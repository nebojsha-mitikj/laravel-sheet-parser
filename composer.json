{
  "name": "nebbo.o/laravel-sheet-parser",
  "description": "A Laravel package to parse data from public Google Sheets and CSV files using a unified interface.",
  "type": "library",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0 || ^11.0",
    "guzzlehttp/guzzle": "^7.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "^8.0 || ^9.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "NebboO\\LaravelSheetParser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NebboO\\LaravelSheetParser\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Nebojsha Mitikj",
      "email": "nebojsha.97.mitikj@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
